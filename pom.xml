		<dependency>
			<groupId>org.eclipse.birt.runtime.3_7_1</groupId>
			<artifactId>org.apache.batik.parser</artifactId>
			<version>1.6.0</version>
		</dependency>
		
		<dependency>
		    <groupId>org.eclipse.birt.runtime.3_7_1</groupId>
